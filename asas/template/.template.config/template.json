{
    "$schema": "http://json.schemastore.org/template",
    "author": "Asas Kit",
    "classifications": [
        "Web",
        "AsasKit"
    ],
    "name": "Asas Kit Starter Template",
    "identity": "AsasKit.Starter",
    "shortName": "asas-kit",
    "tags": {
        "language": "C#",
        "type": "project"
    },
    "sourceName": "AsasKit",
    "preferNameDirectory": true,
    "sources": [
        {
            "modifiers": [
                {
                    "exclude": [
                        "**/[Bb]in/**",
                        "**/[Oo]bj/**",
                        ".vs/**",
                        ".vscode/**",
                        ".git/**",
                        ".idea/**",
                        "**/*.user"
                    ]
                }
            ]
        }
    ],
    "primaryOutputs": [
        {
            "path": "AsasKit.sln"
        }
    ]
}